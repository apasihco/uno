{
    "name": "TGBNoLimit-UNO",
    "description": "A Unobot for telegram.",
    "keywords": [
    "Uno bot",
    "telegram",
    "bot",
    ],
    "env": {
      "token": {
        "description": "Telegram bot token",
        "value": "",
        "required": true
      },
      "admin_list": {
        "description": "Admin id",
        "value": "1471510548",
        "required": true
      },
      "open_lobby": {
        "description": "set true/false",
        "value": "true",
        "required": true
      },
      "enable_translations": {
        "description": "Use traslation?.",
        "value": "false",
        "required": true
      },
      "workers": {
        "description": "worker",
        "value": "8",
        "required": true
      },
      "default_gamemode": {
        "description": "A Pyrogram v2 String Session from @STRINGSEASO_NBOT on Telegram.",
        "value": "fast",
        "required": true
      },
      "waiting_time": {
        "description": "Your Heroku account's API key",
        "value": "120",
        "required": false
      },
      "time_removal_after_skip": {
        "description": "Your heroku app name",
        "value": "20",
        "required": false
      },
      "min_fast_turn_time": {
        "description": "Your Log Group ID, add your bot and promote as an admin with full rights!. Channel ki id mat daal dena bsdk.",
        "value": "15",
        "required": true
      },
      "min_players": {
        "description": "Your Log Group ID, add your bot and promote as an admin with full rights!. Channel ki id mat daal dena bsdk.",
        "value": "2",
        "required": true
      }
    },
    "buildpacks": [
          {
              "url": "heroku/python"
          }
      ],
      "stack": "container"
  }
